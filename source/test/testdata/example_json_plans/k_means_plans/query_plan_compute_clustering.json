{  
  "QUERY_PLAN":{  
    "OPERATOR_NAME":"STORE_TABLE",
    "TABLE_NAME":"CLUSTERED_POINTS",
    "PERSIST_TABLE_ON_DISK":false,
    "LEFT_CHILD":{  
      "OPERATOR_NAME":"GENERIC_GROUPBY",
      "GROUPING_COLUMNS":[  
        {  
          "ATTRIBUTE_REFERENCE":{  
            "COLUMN_NAME":"P_VALUE_0",
            "TABLE_NAME":"POINTS",
            "VERSION":1
          }
        },
        {  
          "ATTRIBUTE_REFERENCE":{  
            "COLUMN_NAME":"P_VALUE_1",
            "TABLE_NAME":"POINTS",
            "VERSION":1
          }
        }
      ],
      "AGGREGATION_SPECIFICATION":[  
        {  
          "AGGREGATION_FUNCTION":"UDF",
          "REDUCE_UDF":{  
            "REDUCE_UDF_TYPE":"ALGEBRAIC",
            "REDUCE_UDF_PAYLOAD_ATTRIBUTES":[  
              {  
                "ATTRIBUTE_TYPE":"double",
                "ATTRIBUTE_NAME":"min_value",
                "ATTRIBUTE_INIT_VALUE":"4000000000"
              },
              {  
                "ATTRIBUTE_TYPE":"OID",
                "ATTRIBUTE_NAME":"ID",
                "ATTRIBUTE_INIT_VALUE":"0"
              }
            ],
            "REDUCE_UDF_OUTPUT_ATTRIBUTES":[  
              {  
                "ATTRIBUTE_TYPE":"OID",
                "ATTRIBUTE_NAME":"CID"
              }
            ],
            "REDUCE_UDF_CODE":[  
              "if(#<hash_entry>.min_value#>computed_var_DIST){",
              "   #<hash_entry>.min_value#=computed_var_DIST;",
              "   #<hash_entry>.ID#=#CENTROIDS.CID#;",
              "}"
            ],
            "REDUCE_UDF_FINAL_CODE":[  
              "#<out>.CID# = #<hash_entry>.ID#;"
            ]
          }
        }
      ],
      "LEFT_CHILD":{  
        "OPERATOR_NAME":"MAP_UDF",
        "MAP_UDF_OUTPUT_ATTRIBUTES":[  
          {  
            "ATTRIBUTE_TYPE":"double",
            "ATTRIBUTE_NAME":"DIST"
          }
        ],
        "MAP_UDF_CODE":[  
          "#<OUT>.DIST#=sqrt(pow(#POINTS.P_VALUE_0#-#CENTROIDS.C_VALUE_0#,2)+pow(#POINTS.P_VALUE_1#-#CENTROIDS.C_VALUE_1#,2));"
        ],
        "LEFT_CHILD":{  
          "OPERATOR_NAME":"CROSS_JOIN",
          "LEFT_CHILD":{  
            "OPERATOR_NAME":"TABLE_SCAN",
            "TABLE_NAME":"POINTS"
          },
          "RIGHT_CHILD":{  
            "OPERATOR_NAME":"TABLE_SCAN",
            "TABLE_NAME":"CENTROIDS"
          }
        },
        "RIGHT_CHILD":null
      },
      "RIGHT_CHILD":null
    },
    "RIGHT_CHILD":null
  }
}
